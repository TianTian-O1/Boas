{
  "name": "boas",
  "version": "1.0.0",
  "description": "High-performance AI compiler with Python syntax",
  "main": "build/boas",
  "repository": {
    "type": "git",
    "url": "https://github.com/boas-project/boas.git"
  },
  "keywords": [
    "compiler",
    "python",
    "npu",
    "ascend",
    "ai",
    "mlir",
    "llvm",
    "high-performance"
  ],
  "author": "BOAS Development Team",
  "license": "MIT",
  "dependencies": {
    "llvm": "20.0.0",
    "cann": ">=6.0.0"
  },
  "build": {
    "type": "cmake",
    "requires": [
      "cmake>=3.20",
      "gcc>=9.0",
      "python>=3.8"
    ]
  },
  "platforms": [
    "linux-aarch64",
    "linux-x86_64"
  ],
  "hardware": {
    "supported": [
      "ascend-910a",
      "ascend-910b",
      "ascend-310p"
    ]
  }
}